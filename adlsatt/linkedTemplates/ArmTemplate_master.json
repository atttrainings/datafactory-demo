{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adlsatt"
		},
		"lsdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdatalake'"
		},
		"lssqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lssqldb'"
		},
		"lsstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsstorage'"
		},
		"lsdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsag2222.dfs.core.windows.net/"
		},
		"storage-trigger-create_properties_pipeline1_parameters_inputFileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"storage-trigger-create_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/69456469-0ef6-474c-a4b4-adb1415fdd2f/resourceGroups/rgdemo/providers/Microsoft.Storage/storageAccounts/saag687788hhnnhh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adlsatt_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsdatalake_accountKey": {
						"value": "[parameters('lsdatalake_accountKey')]"
					},
					"lssqldb_connectionString": {
						"value": "[parameters('lssqldb_connectionString')]"
					},
					"lsstorage_connectionString": {
						"value": "[parameters('lsstorage_connectionString')]"
					},
					"lsdatalake_properties_typeProperties_url": {
						"value": "[parameters('lsdatalake_properties_typeProperties_url')]"
					},
					"storage-trigger-create_properties_pipeline1_parameters_inputFileName": {
						"value": "[parameters('storage-trigger-create_properties_pipeline1_parameters_inputFileName')]"
					},
					"storage-trigger-create_properties_typeProperties_scope": {
						"value": "[parameters('storage-trigger-create_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}